#basic python syntax
#basic slash

print("hello\
world")
#to print hello world with back slash i.e continuation sign


#triple quotes

print(""" ()...()
(o o)
/( ..)\
/""\
""")
#to print text art with triple quotes

print(""" This program is written by rhythum""")
#to print doc string using triple quotes

#string inside the quotes 

print("hello world")
#to print hello world with double quotes

print('hello world')
#to print hello world with single quotes

print('python\'s world')
#to print python world 

#escape sequence of string

print("hello\n python")
#to print hello and python in next line

print("hello\t python")
#to print hello python with tab space

print("hello\a python")
#to print hello python with alert sound 



#arithmetic operators 

print(10-20)
print(10+20)
print(10/2)
print(10/3)
print(10%3)
print(10//3)

#membership operators 

str1 = "python programming"
print("a" in str1)

str1 = "python programming"
print("c" in str1)
